{
  "name": "pepsa-component",
  "version": "0.0.4",
  "description": "A componentsjs component to run the PePSA middleware on the solid community server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Jesse Geens <jesse.geens@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:jessegeens/pepsa-component.git",
  "files": [
    "config",
    "dist"
  ],
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/pepsa-component",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/pepsa-component/^0.0.4/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/pepsa-component/^0.0.4/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/pepsa-component/^0.0.4/config/": "config/css-configs/",
    "https://linkedsoftwaredependencies.org/bundles/npm/pepsa-component/^0.0.4/dist/": "dist/"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -i .componentsignore",
    "build:ts": "tsc",
    "lint": "eslint . --cache",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "componentsjs-generator": "^2.6.1",
    "gts": "^3.1.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@solid/community-server": "^2.0.1",
    "componentsjs": "^4.5.0"
  }
}
